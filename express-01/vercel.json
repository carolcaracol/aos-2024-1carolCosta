{
  "version": 2,
  "builds": [
    {
      "src": "express-01/src/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "express-01/src/api/index.js"
    }
  ]
}
